{
  "compilerOptions": {
    "baseUrl": "./src", 
    "paths": {
      "@utils/*": ["utils/*"], 
      "@commands/*": ["commands/*"], 
      "@server/*": ["server/*"], 
      "@modules/*": ["modules/*"]
    },
    "target": "es2017",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "outDir": "./dist",
  },
  "include": [
    "src/*.ts",
  ],
  "exclude": [
    "node_modules"
  ]
}